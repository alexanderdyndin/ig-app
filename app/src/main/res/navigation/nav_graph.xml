<?xml version="1.0" encoding="utf-8"?>
<navigation
    android:id="@+id/nav_graph"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/groupListFragment"
        android:name="com.intergroupapplication.presentation.feature.grouplist.view.GroupListFragment"
        android:label="GroupListFragment"
        tools:layout="@layout/fragment_group_list">
        <action
            android:id="@+id/action_groupListFragment2_to_newsFragment2"
            app:destination="@id/newsFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_groupListFragment2_to_createGroupActivity"
            app:destination="@id/createGroupFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_groupListFragment2_to_groupActivity"
            app:destination="@id/groupFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_groupListFragment2_to_commentsDetailsActivity"
            app:destination="@id/commentsDetailsFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_groupListFragment2_self"
            app:destination="@id/groupListFragment"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_groupListFragment2_to_loginActivity2"
            app:destination="@id/loginFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="true"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_groupListFragment2_to_confirmationMailActivity"
            app:destination="@id/confirmationMailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="true"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_groupListFragment2_to_audioListFragment"
            app:destination="@id/audioListFragment" />
    </fragment>
    <fragment
        android:id="@+id/newsFragment"
        android:name="com.intergroupapplication.presentation.feature.news.view.NewsFragment"
        android:label="NewsFragment"
        tools:layout="@layout/fragment_news">
        <action
            android:id="@+id/action_newsFragment2_to_groupListFragment2"
            app:destination="@id/groupListFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_newsFragment2_to_groupActivity"
            app:destination="@id/groupFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_newsFragment2_to_commentsDetailsActivity"
            app:destination="@id/commentsDetailsFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_newsFragment2_to_loginActivity"
            app:destination="@id/loginFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="true"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_newsFragment2_self"
            app:destination="@id/newsFragment"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_newsFragment2_to_confirmationMailActivity"
            app:destination="@id/confirmationMailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="true"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_newsFragment2_to_createUserProfileActivity"
            app:destination="@id/createUserProfileFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="false"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_newsFragment2_to_imageFragment"
            app:destination="@id/imageFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />
        <action
            android:id="@+id/action_newsFragment2_to_audioListFragment"
            app:destination="@id/audioListFragment" />
    </fragment>
    <fragment
        android:id="@+id/splashFragment"
        android:name="com.intergroupapplication.presentation.feature.splash.SplashFragment"
        android:label="SplashFragment"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splashActivity_to_registrationActivity"
            app:destination="@id/registrationFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="true"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_splashActivity_to_AgreementsFragment2"
            app:destination="@id/AgreementsFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="true"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_splashActivity_to_newsFragment2"
            app:destination="@id/newsFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="true"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/registrationFragment"
        android:name="com.intergroupapplication.presentation.feature.registration.view.RegistrationFragment"
        android:label="RegistrationFragment"
        tools:layout="@layout/fragment_registration" >
        <action
            android:id="@+id/action_registrationActivity_to_loginActivity2"
            app:destination="@id/loginFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />
        <action
            android:id="@+id/action_registrationActivity_to_confirmationMailActivity2"
            app:destination="@id/confirmationMailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.intergroupapplication.presentation.feature.login.view.LoginFragment"
        android:label="LoginFragment"
        tools:layout="@layout/fragment_login">
        <action
            android:id="@+id/action_loginActivity_to_splashActivity"
            app:destination="@id/splashFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_loginActivity_to_registrationActivity"
            app:destination="@id/registrationFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_loginActivity_to_recoveryPasswordActivity"
            app:destination="@id/recoveryPasswordFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_loginActivity_to_confirmationMailActivity"
            app:destination="@id/confirmationMailFragment" />
        <action
            android:id="@+id/action_loginActivity_to_createUserProfileActivity"
            app:destination="@id/createUserProfileFragment" />
    </fragment>
    <fragment
        android:id="@+id/confirmationMailFragment"
        android:name="com.intergroupapplication.presentation.feature.confirmationmail.view.ConfirmationMailFragment"
        android:label="ConfirmationMailFragment"
        tools:layout="@layout/fragment_confirmation_mail">
        <argument
            android:name="entity"
            app:argType="string"
            app:nullable="true" />
        <action
            android:id="@+id/action_confirmationMailActivity_to_createUserProfileActivity"
            app:destination="@id/createUserProfileFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_confirmationMailActivity_to_registrationActivity"
            app:destination="@id/registrationFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="false"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/AgreementsFragment"
        android:name="com.intergroupapplication.presentation.feature.agreements.view.AgreementsFragment"
        android:label="AgreementsFragment"
        tools:layout="@layout/fragment_agreements">
        <action
            android:id="@+id/action_AgreementsFragment2_to_webFragment"
            app:destination="@id/webFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_AgreementsFragment2_to_splashActivity"
            app:destination="@id/splashFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/webFragment"
        android:name="com.intergroupapplication.presentation.feature.web.WebFragment"
        android:label="WebFragment"
        tools:layout="@layout/fragment_web">
        <argument
            android:name="PATH"
            app:argType="string" />
        <argument
            android:name="TITLE"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/createUserProfileFragment"
        android:name="com.intergroupapplication.presentation.feature.createuserprofile.view.CreateUserProfileFragment"
        android:label="CreateUserProfileFragment"
        tools:layout="@layout/fragment_create_user_profile">
        <action
            android:id="@+id/action_createUserProfileActivity_to_splashActivity"
            app:destination="@id/splashFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/createGroupFragment"
        android:name="com.intergroupapplication.presentation.feature.creategroup.view.CreateGroupFragment"
        android:label="CreateGroupFragment"
        tools:layout="@layout/fragment_group_create" />
    <fragment
        android:id="@+id/recoveryPasswordFragment"
        android:name="com.intergroupapplication.presentation.feature.recoveryPassword.view.RecoveryPasswordFragment"
        android:label="RecoveryPasswordFragment"
        tools:layout="@layout/fragment_recovery_password"/>
    <fragment
        android:id="@+id/groupFragment"
        android:name="com.intergroupapplication.presentation.feature.group.view.GroupFragment"
        android:label="GroupFragment"
        tools:layout="@layout/fragment_group">
        <argument
            android:name="groupId"
            app:argType="string" />
        <action
            android:id="@+id/action_groupActivity_to_CreatePostFragment"
            app:destination="@id/CreatePostFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_groupActivity_to_commentsDetailsActivity"
            app:destination="@id/commentsDetailsFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <argument
            android:name="isGroupCreatedNow"
            app:argType="boolean"
            android:defaultValue="false" />
        <action
            android:id="@+id/action_groupActivity_to_userListFragment"
            app:destination="@id/userListFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_groupActivity_to_imageFragment"
            app:destination="@id/imageFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />
        <argument
            android:name="owner"
            app:argType="string"
            app:nullable="true" />
        <action
            android:id="@+id/action_groupActivity_to_editPostFragment"
            app:destination="@id/editPostFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/CreatePostFragment"
        android:name="com.intergroupapplication.presentation.feature.createpost.view.CreatePostFragment"
        android:label="CreatePostFragment"
        tools:layout="@layout/fragment_create_post">
        <argument
            android:name="group_id"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/commentsDetailsFragment"
        android:name="com.intergroupapplication.presentation.feature.commentsdetails.view.CommentsDetailsFragment"
        android:label="CommentsDetailsFragment"
        tools:layout="@layout/fragment_comments_details">
        <argument
            android:name="comment_post"
            app:argType="com.intergroupapplication.domain.entity.GroupPostEntity$PostEntity"
            app:nullable="true" />
        <action
            android:id="@+id/action_commentsDetailsActivity_to_groupActivity"
            app:destination="@id/groupFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <argument
            android:name="post_id"
            app:argType="string"
            app:nullable="true" />
        <action
            android:id="@+id/action_commentsDetailsActivity_to_imageFragment"
            app:destination="@id/imageFragment" />
        <argument
            android:name="page"
            app:argType="string"
            android:defaultValue="1" />
    </fragment>
    <fragment
        android:id="@+id/userListFragment"
        android:name="com.intergroupapplication.presentation.feature.userlist.view.UserListFragment"
        android:label="UserListFragment"
        tools:layout="@layout/fragment_user_list">
        <argument
            android:name="group_id"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/imageFragment"
        android:name="com.intergroupapplication.presentation.feature.image.view.ImageFragment"
        android:label="ImageFragment"
        tools:layout="@layout/fragment_image">
        <argument
            android:name="images"
            app:argType="com.intergroupapplication.domain.entity.FileEntity[]" />
        <argument
            android:name="selectedId"
            app:argType="integer"
            android:defaultValue="0" />
    </fragment>
    <action android:id="@+id/action_global_newsFragment2" app:destination="@id/newsFragment"/>
    <fragment
        android:id="@+id/audioListFragment"
        android:name="com.intergroupapplication.presentation.feature.audiolist.view.AudioListFragment"
        android:label="AudioListFragment"
        tools:layout="@layout/fragment_audios_list">
        <action
            android:id="@+id/action_audioListFragment_self"
            app:destination="@id/audioListFragment" />
    </fragment>
    <fragment
        android:id="@+id/editPostFragment"
        android:name="com.intergroupapplication.presentation.feature.editpost.view.EditPostFragment"
        android:label="EditPostFragment"
        tools:layout="@layout/fragment_create_post">
        <argument
            android:name="group_id"
            app:argType="string" />
        <argument
            android:name="group_post_entity_key"
            app:argType="com.intergroupapplication.domain.entity.GroupPostEntity$PostEntity"
            app:nullable="false"/>
    </fragment>
</navigation>