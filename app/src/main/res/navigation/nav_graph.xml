<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/groupListFragment"
        android:name="com.intergroupapplication.presentation.feature.grouplist.view.GroupListFragment"
        android:label="GroupListFragment"
        tools:layout="@layout/fragment_group_list">
        <action
            android:id="@+id/action_groupListFragment_to_newsFragment"
            app:destination="@id/newsFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_groupListFragment_to_createGroupFragment"
            app:destination="@id/createGroupFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_groupListFragment_to_groupFragment"
            app:destination="@id/groupFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_groupListFragment_to_commentsDetailsFragment"
            app:destination="@id/commentsDetailsFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_groupListFragment_self"
            app:destination="@id/groupListFragment"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_groupListFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="true"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_groupListFragment_to_confirmationMailFragment"
            app:destination="@id/confirmationMailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="true"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_groupListFragment_to_audioListFragment"
            app:destination="@id/audioListFragment" />
    </fragment>
    <fragment
        android:id="@+id/newsFragment"
        android:name="com.intergroupapplication.presentation.feature.news.view.NewsFragment"
        android:label="NewsFragment"
        tools:layout="@layout/fragment_news">
        <action
            android:id="@+id/action_newsFragment_to_groupListFragment"
            app:destination="@id/groupListFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_newsFragment_to_groupFragment"
            app:destination="@id/groupFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_newsFragment_to_commentsDetailsFragment"
            app:destination="@id/commentsDetailsFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_newsFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="true"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_newsFragment_self"
            app:destination="@id/newsFragment"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_newsFragment_to_confirmationMailFragment"
            app:destination="@id/confirmationMailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="true"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_newsFragment_to_createUserProfileFragment"
            app:destination="@id/createUserProfileFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="false"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_newsFragment_to_imageFragment"
            app:destination="@id/imageFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />
        <action
            android:id="@+id/action_newsFragment_to_audioListFragment"
            app:destination="@id/audioListFragment" />
    </fragment>
    <fragment
        android:id="@+id/splashFragment"
        android:name="com.intergroupapplication.presentation.feature.splash.SplashFragment"
        android:label="SplashFragment"
        tools:layout="@layout/fragment_splash">
        <action
            android:id="@+id/action_splashFragment_to_registrationFragment"
            app:destination="@id/registrationFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="true"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_splashFragment_to_AgreementsFragment"
            app:destination="@id/AgreementsFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="true"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_splashFragment_to_newsFragment"
            app:destination="@id/newsFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="true"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/registrationFragment"
        android:name="com.intergroupapplication.presentation.feature.registration.view.RegistrationFragment"
        android:label="RegistrationFragment"
        tools:layout="@layout/fragment_registration">
        <action
            android:id="@+id/action_registrationFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:enterAnim="@anim/slide_in"
            app:popExitAnim="@anim/slide_out" />
        <action
            android:id="@+id/action_registrationFragment_to_confirmationMailFragment"
            app:destination="@id/confirmationMailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.intergroupapplication.presentation.feature.login.view.LoginFragment"
        android:label="LoginFragment"
        tools:layout="@layout/fragment_login">
        <action
            android:id="@+id/action_loginFragment_to_splashFragment"
            app:destination="@id/splashFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_loginActivity_to_registrationActivity"
            app:destination="@id/registrationFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_loginFragment_to_recoveryPasswordFragment"
            app:destination="@id/recoveryPasswordFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_loginFragment_to_confirmationMailFragment"
            app:destination="@id/confirmationMailFragment" />
        <action
            android:id="@+id/action_loginActivity_to_createUserProfileActivity"
            app:destination="@id/createUserProfileFragment" />
    </fragment>
    <fragment
        android:id="@+id/confirmationMailFragment"
        android:name="com.intergroupapplication.presentation.feature.confirmationmail.view.ConfirmationMailFragment"
        android:label="ConfirmationMailFragment"
        tools:layout="@layout/fragment_confirmation_mail">
        <argument
            android:name="entity"
            app:argType="string"
            app:nullable="true" />
        <action
            android:id="@+id/action_confirmationMailFragment_to_createUserProfileFragment"
            app:destination="@id/createUserProfileFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_confirmationMailFragment_to_registrationFragment"
            app:destination="@id/registrationFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="false"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/AgreementsFragment"
        android:name="com.intergroupapplication.presentation.feature.agreements.view.AgreementsFragment"
        android:label="AgreementsFragment"
        tools:layout="@layout/fragment_agreements">
        <action
            android:id="@+id/action_AgreementsFragment_to_splashFragment"
            app:destination="@id/splashFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_AgreementsFragment_to_agreementFragment"
            app:destination="@id/agreementFragment"
            app:enterAnim="@anim/slide_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/slide_out" />
    </fragment>
    <fragment
        android:id="@+id/createUserProfileFragment"
        android:name="com.intergroupapplication.presentation.feature.createuserprofile.view.CreateUserProfileFragment"
        android:label="CreateUserProfileFragment"
        tools:layout="@layout/fragment_create_user_profile">
        <action
            android:id="@+id/action_createUserProfileFragment_to_splashFragment"
            app:destination="@id/splashFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/createGroupFragment"
        android:name="com.intergroupapplication.presentation.feature.creategroup.view.CreateGroupFragment"
        android:label="CreateGroupFragment"
        tools:layout="@layout/fragment_group_create">
        <action
            android:id="@+id/action_createGroupFragment_to_groupFragment"
            app:destination="@id/groupFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/recoveryPasswordFragment"
        android:name="com.intergroupapplication.presentation.feature.recoveryPassword.view.RecoveryPasswordFragment"
        android:label="RecoveryPasswordFragment"
        tools:layout="@layout/fragment_recovery_password" />
    <fragment
        android:id="@+id/groupFragment"
        android:name="com.intergroupapplication.presentation.feature.group.view.GroupFragment"
        android:label="GroupFragment"
        tools:layout="@layout/fragment_group">
        <argument
            android:name="groupId"
            app:argType="string" />
        <action
            android:id="@+id/action_groupFragment_to_createPostFragment"
            app:destination="@id/CreatePostFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_groupFragment_to_commentsDetailsFragment"
            app:destination="@id/commentsDetailsFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_groupFragment_to_userListFragment"
            app:destination="@id/userListFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_groupFragment_to_imageFragment"
            app:destination="@id/imageFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />
        <argument
            android:name="owner"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="group"
            app:argType="com.intergroupapplication.domain.entity.GroupEntity$Group"
            app:nullable="true" />
        <action
            android:id="@+id/action_groupFragment_to_editPostFragment"
            app:destination="@id/editPostFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/CreatePostFragment"
        android:name="com.intergroupapplication.presentation.feature.createpost.view.CreatePostFragment"
        android:label="CreatePostFragment"
        tools:layout="@layout/fragment_create_post">
        <argument
            android:name="group_id"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/commentsDetailsFragment"
        android:name="com.intergroupapplication.presentation.feature.commentsdetails.view.CommentsDetailsFragment"
        android:label="CommentsDetailsFragment"
        tools:layout="@layout/fragment_comments_details">
        <argument
            android:name="comment_post"
            app:argType="com.intergroupapplication.domain.entity.GroupPostEntity$PostEntity"
            app:nullable="true" />
        <action
            android:id="@+id/action_commentsDetailsFragment_to_groupFragment"
            app:destination="@id/groupFragment"
            app:enterAnim="@anim/slide_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/slide_out" />
        <argument
            android:name="post_id"
            app:argType="string"
            app:nullable="true" />
        <action
            android:id="@+id/action_commentsDetailsFragment_to_imageFragment"
            app:destination="@id/imageFragment" />
        <argument
            android:name="page"
            android:defaultValue="1"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/userListFragment"
        android:name="com.intergroupapplication.presentation.feature.userlist.view.UserListFragment"
        android:label="UserListFragment"
        tools:layout="@layout/fragment_user_list">
        <argument
            android:name="group_id"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/imageFragment"
        android:name="com.intergroupapplication.presentation.feature.image.view.ImageFragment"
        android:label="ImageFragment"
        tools:layout="@layout/fragment_image">
        <argument
            android:name="images"
            app:argType="com.intergroupapplication.domain.entity.FileEntity[]" />
        <argument
            android:name="selectedId"
            android:defaultValue="0"
            app:argType="integer" />
    </fragment>
    <action
        android:id="@+id/action_global_newsFragment"
        app:destination="@id/newsFragment"
        app:popUpTo="@id/nav_graph" />
    <fragment
        android:id="@+id/audioListFragment"
        android:name="com.intergroupapplication.presentation.feature.audiolist.view.AudioListFragment"
        android:label="AudioListFragment"
        tools:layout="@layout/fragment_audios_list">
        <action
            android:id="@+id/action_audioListFragment_self"
            app:destination="@id/audioListFragment" />
    </fragment>
    <fragment
        android:id="@+id/editPostFragment"
        android:name="com.intergroupapplication.presentation.feature.editpost.view.EditPostFragment"
        android:label="EditPostFragment"
        tools:layout="@layout/fragment_create_post">
        <argument
            android:name="group_id"
            app:argType="string" />
        <argument
            android:name="group_post_entity_key"
            app:argType="com.intergroupapplication.domain.entity.GroupPostEntity$PostEntity"
            app:nullable="false" />
    </fragment>
    <action
        android:id="@+id/action_global_groupListFragment"
        app:destination="@id/groupListFragment"
        app:popUpTo="@id/nav_graph" />
    <action
        android:id="@+id/action_global_splashActivity"
        app:destination="@id/splashFragment"
        app:popUpTo="@id/nav_graph" />
    <action
        android:id="@+id/action_global_AgreementsFragment"
        app:destination="@id/AgreementsFragment"
        app:popUpTo="@id/nav_graph" />
    <action
        android:id="@+id/action_global_createUserProfileActivity"
        app:destination="@id/createUserProfileFragment"
        app:popUpTo="@id/nav_graph" />
    <action
        android:id="@+id/action_global_registrationFragment"
        app:destination="@id/registrationFragment"
        app:popUpTo="@id/nav_graph" />
    <action
        android:id="@+id/action_global_confirmationMailFragment"
        app:destination="@id/confirmationMailFragment"
        app:popUpTo="@id/nav_graph" />
    <action
        android:id="@+id/action_global_audioListFragment"
        app:destination="@id/audioListFragment"
        app:popUpTo="@id/nav_graph" />
    <action
        android:id="@+id/action_global_loginFragment"
        app:destination="@id/loginFragment"
        app:popUpTo="@id/nav_graph" />
    <fragment
        android:id="@+id/agreementFragment"
        android:name="com.intergroupapplication.presentation.feature.agreement.view.AgreementFragment"
        android:label="AgreementFragment"
        tools:layout="@layout/fragment_agreement">
        <argument
            android:name="resId"
            app:argType="integer" />
    </fragment>
</navigation>